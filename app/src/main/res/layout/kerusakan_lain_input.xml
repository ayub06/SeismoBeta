                android:id="@+id/kerusakan_lain_irigasi" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="unit"
                android:textSize="16sp"/>
        </LinearLayout>
    </GridLayout>

</LinearLayout>